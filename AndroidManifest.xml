    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.attendance_app.MainMenu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.example.attendance_app.Mark_Attendance_Activity"
            android:label="attendance" >
        </activity>
        <activity
            android:name="com.example.attendance_app.View_Attendance_Activity"
            android:label="attendance" >
        </activity>
        <activity
            android:name="com.example.attendance_app.View_Login"
            android:label="attendance" >
        </activity>
        <activity
            android:name="com.example.attendance_app.Mark_login"
            android:label="attendance" >
        </activity>
    </application>

</manifest>
